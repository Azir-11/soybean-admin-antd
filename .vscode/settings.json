{
  "cSpell.words": [
    "Antd",
    "antv",
    "clsx",
    "colord",
    "consola",
    "EDITMSG",
    "espree",
    "execa",
    "hexcode",
    "iconify",
    "INDEXEDDB",
    "jiti",
    "kolorist",
    "Laba",
    "localforage",
    "LOCALSTORAGE",
    "nocheck",
    "nprogress",
    "ofetch",
    "preflights",
    "sider",
    "tada",
    "Uncapitalize",
    "unocss",
    "unplugin",
    "VITE",
    "vitepress",
    "vueuse",
    "WEBSQL",
    "wechat"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.fontLigatures": true,
  "editor.formatOnSave": false,
  "editor.quickSuggestions": {
    "strings": true
  },
  "editor.tabSize": 2,
  "files.associations": {
    "*.env.*": "dotenv",
    "*.svg": "html"
  },
  "files.eol": "\n",
  "unocss.root": ["./"],
  "[html][css][less][scss][sass][markdown][yaml][yml][json][jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "i18n-ally.localesPaths": ["src/locales", "src/locales/lang"]
}
